---
import { SITE } from '@config';

import Layout from '../../layouts/Layout.astro';
import PostList from '../../layouts/PostList.astro';

let pageTitle = 'University Projects';
---

<Layout
	pageTitle={pageTitle + ' of ' + SITE.titleShort}
	pageDescr={"A list of my university projects from my time at HfG (University of Applied Sciences Schwäbisch Gmünd) between 2016 and 2020. Starting with 'Aral Sea', an interactive exhibit, until 'Destinations', my bachelor thesis."}>
	<PostList collectionName={'projects'} navTitle="Projects" pageTitle={pageTitle} />
</Layout>
